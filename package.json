{
  "name": "sunrinthon",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.18.0",
    "mongoose": "^5.1.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-preload": "^0.5.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "build-css": "node-less-chokidar src --source-map-contents",
    "watch-css": "node-less-chokidar src --source-map-contents --watch",
    "start": "npm run build-css && run-p -ncr watch-css start-ts",
    "start-ts": "cross-env BROWSER=false PORT=3001 react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "classnames": "^2.2.6",
    "cross-env": "^5.1.6",
    "less-loader": "^4.1.0",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewire-less-modules": "^1.3.0",
    "react-app-rewired": "^1.5.2",
    "react-router-dom": "^4.3.1",
    "typed-css-modules": "^0.3.4",
    "typescript": "^2.9.1",
    "typings-for-css-modules-loader": "^1.7.0"
  }
}
